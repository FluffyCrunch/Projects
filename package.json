    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "socket.io": "^4.8.1"
